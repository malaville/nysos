{
  "name": "nysos-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "stageyaml": "cp app.yaml ./dist/app.yaml && sed -i '5iservice: stage' ./dist/app.yaml",
    "prodymal": "cp app.yaml ./dist/app.yaml",
    "build": "rm -rf dist/ && ng build --aot --prod --output-hashing=all",
    "gaedeploy": "cd dist && yes | gcloud app deploy --project=nysos-289715 && cd ..",
    "gaedeploy-": "cd dist && yes | gcloud app deploy --project=nysos-289715 --no-promote  && cd ..",
    "deploy+": "npm run -s build && npm run -s prodymal &&  npm run -s gaedeploy",
    "deploy-": "npm run -s build && npm run -s prodymal &&  npm run -s gaedeploy-",
    "deploystage": "npm run -s build && npm run -s stageyaml &&  npm run -s gaedeploy",
    "analyze": "ng run nysos-frontend:build:production --stats-json=true --statsJson=true",
    "webpack-analyzer": "webpack-bundle-analyzer dist/nysos-frontend/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/cdk": "~10.2.5",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/material": "~10.2.5",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "@angular/service-worker": "~10.2.0",
    "angularx-social-login": "^3.2.2",
    "cytoscape": "^3.16.0",
    "cytoscape-edgehandles": "^3.6.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@types/cytoscape": "^3.14.7",
    "@types/cytoscape-edgehandles": "^3.6.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.3",
    "webpack-bundle-analyzer": "^3.9.0"
  }
}
